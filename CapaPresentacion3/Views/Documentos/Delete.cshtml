@model CapaEntidades.EnvioDocumento

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Departamento</h2>

<h3>Seguro que deseas eliminar este Departamento?</h3>
<hr />
<div>
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.TipoDocumento)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TipoDocumento)
        </dd>

        <dt>
            <span>Usuario</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Usuarios.Nombre)
        </dd>

        <dt>
            <span>Origen</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Departamentos1.Nombre)
        </dd>

        <dt>
            <span>Destino</span>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Departamentos.Nombre)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Secuencia)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Secuencia)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Fecha)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar Documento" class="btn btn-danger btn-block w-25 mb-3" name="usuario" />
        </div>
        @Html.ActionLink("Volver", "Lista")
    }
</div>
